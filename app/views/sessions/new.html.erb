
<h2>Sign in</h2>

<div>
  <%= form_tag "/signin" do %>
    <div>
      <%= label_tag :name %>
      <%= text_field_tag :name, params[:name] %>
    </div>
     <br>
    <div>
      <%= label_tag  :password %>
      <%= password_field_tag  :password, params[:password]  %>
    </div>
      <br>
      <%= submit_tag "Submit" %>
  <%end%>
</div>